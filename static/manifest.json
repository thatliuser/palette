{
    "manifest_version": 2,
    "name": "palette",
    "author": "Eric",
    "version": "1.0.0",
    "description": "A Firefox extension designed to make using Canvas more efficient.",
    "content_scripts": [
        {
            "matches": [
                "https://login.canvas.ucdavis.edu/"
            ],
            "js": [
                "goto-login.js"
            ]
        },
        {
            "matches": [
                "https://canvas.ucdavis.edu/*"
            ],
            "js": [
                "add-keybinds.js"
            ]
        },
        {
            "matches": [
                "https://canvas.ucdavis.edu/*"
            ],
            "css": [
                "style.css"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://cas.ucdavis.edu/cas/login*"
            ],
            "js": [
                "autosubmit.js"
            ]
        }
    ],
    "icons": {
        "96": "96x96.png"
    }
}